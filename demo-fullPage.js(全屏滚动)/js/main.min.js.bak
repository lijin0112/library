 * Detect scroll direction
 * @param  string  jQuery selector of target element
 */
var detectScrollDirection = function(target) {
    $(target).on('DOMMouseScroll mousewheel', function(e) {

        // Scroll Down
        if( e.originalEvent.detail > 0 || e.originalEvent.wheelDelta < 0 ) {
            console.log("scroll down");
        }
        // Scroll UP
        else if ( e.originalEvent.detail < 0 || e.originalEvent.wheelDelta > 0 ) {
            console.log("scroll up");
        }
    });
};


/*
 * Control a target element's scroll from a specified source object
 * Takes into account OS X "natural" scroll (inverted)
 *
 * @param  string  jQuery selector of the source
 * @param  string  jQuery selector of target object
 * @param  boolean jScrollPane
 *
 */
var scrollTarget = function(source, target) {
    // target to be scrolled
    var tar = $(target);

    // new target to scroll to
    var scrollY = tar.scrollTop();

    // when mousewheel on source
    //$(source).on('mousewheel wheel DOMMouseScroll', function(e){
    $(source).on('mousewheel', function(e){

        // prevent default behavior
        e.preventDefault();

        // console.log(e);

        // console.log('wheel');
        // console.log('e.originalEvent.deltaY ' + e.originalEvent.deltaY);
        // console.log('e.originalEvent.wheelDeltaY ' + e.originalEvent.wheelDeltaY);

        // // log the values
        // console.log('mousewheel');
        // console.log('e.deltaX ' + e.deltaX);
        // console.log('e.deltaY ' + e.deltaY);
        // console.log('e.deltaFactor ' + e.deltaFactor);


        // console.log('e.originalEvent.detail ' + e.originalEvent.detail);
        // console.log('e.originalEvent.wheelDelta ' + e.originalEvent.wheelDelta);

        // if scrolling DOWN && e.deltaY === negative value
            // invert them for "normal" scrolling
        //if ( (e.originalEvent.detail > 0 || e.originalEvent.wheelDelta < 0) && (e.deltaY < 0) ) {
        if ( (e.originalEvent.deltaY < 0) && (e.deltaY > 0) ) {
            // invert values
            scrollY = -(e.deltaY * e.deltaFactor);
            // console.log("inverted scroll " + scrollY);

            // set scrollY to amount scrolled + current scrollTop
            scrollY += tar.scrollTop();
            // console.log("inverted scroll + scrollTop " + scrollY);
        }
        // if scrolling UP && e.deltaY === negative value
            // invert it for "normal" scrolling
        else if ( (e.originalEvent.deltaY > 0) && (e.deltaY < 0) ) {
            // invert values
            scrollY = -(e.deltaY * e.deltaFactor);
            // console.log("inverted scroll " + scrollY);

            // set scrollY to amount scrolled + current scrollTop
            scrollY += tar.scrollTop();
            // console.log("inverted scroll + scrollTop " + scrollY);
        }
        else {
            // set scrollY to amount scrolled + current scrollTop
            scrollY = ( (e.deltaY * e.deltaFactor) + tar.scrollTop() );
            // console.log("scroll + scrollTop " + scrollY);
        }

        // if (!jScrollPane){
            // scroll target to scrollY position
            tar.scrollTop(scrollY).scrollLeft(e.deltaX);
        // }
        //else {
            // scroll jScrollPane

        //}
        // console.log("New tar scrollTop " + tar.scrollTop());

    });
};

