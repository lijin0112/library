        first_tumblr_request = true; // only init Masonry on first request

    var tumblr_posts_per_request = 10,
        // tumblr_api_key = "eErxTTSUqTvaVPG1GnvFXP0PQcRVUu1Yqo0nCD0QpHrpfEQQm3",
        // tumblr_api_root_path = "https://api.tumblr.com/v2/blog/stagingtest888.tumblr.com/",
        tumblr_api_key = "EIExoNvYVTC7fbFB5zF5BDHIC94mfX3zEkB8FLKjsBz3IVkjba", // prod
        tumblr_api_root_path = "https://api.tumblr.com/v2/blog/burntmovie2015.tumblr.com/", // prod
        tumblr_total_posts;

    //
    // Set grid variable for Masonry
    //
    var $grid = $('.grid');


    //
    // Filter / Tag events
    //
    $(".tumblr-filter").each(function() {
        $(this).click(function(e){
            e.preventDefault();
            handleFilter($(this));
        });
    });


    //
    // Infinite Scroll detection
    // Detects bottom of .section_gallery .inner
