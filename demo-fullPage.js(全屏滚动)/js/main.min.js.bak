                    // append class to parent for main volume icon update
                    if (volume === 1) {
                        $(this).closest('.mejs-volume-button').addClass('volume--high').removeClass('volume--medium volume--low');
                    } else if (volume === 0.5) {
                        $(this).closest('.mejs-volume-button').addClass('volume--medium').removeClass('volume--high volume--low');
