        // if currentWidth >= 1024 && targetWidth < 1024
          // user resized their browser
        if ( currentWidth >= 1024 && targetWidth < 1024 && typeof orientation === 'undefined' ) {

            // destroy FullPage.js
            $.fn.fullpage.destroy('all');

            // if user resized window on desktop
            // destroy jScrollPane
            if ( $recipeCopy.data('jsp') ) {
                $recipeCopy.each(function(){
                    $(this).data('jsp').destroy();
                });
            }
            
            // Unbind .nav listeners
            $('.nav-primary__menu-item > a').off('click.burnt.lg');

            // Unbind window hashchange listeners
            $(window).off('hashchange', reactToHash);

            // Stop playing homeVideo
            homeVideo.pause();
            var homeVideoPlayer = $('#video-home')[0].player;
            homeVideoPlayer.remove();
            $('#video-home').remove();

            // Init Mobile Functionality
            initMobile();

            currentWidth = window.innerWidth;
        }

        // if window has been resized down from desktop
        // the resized up, display a "refresh" message
        if ( currentWidth <= 1024 && targetWidth > 1024 && reload === false && typeof orientation === 'undefined' ) {
            var $reload = $('<div>');
            $reload.addClass('reload-message absolute-full').html('<p class="title--alt  center-vertical">Please reload this page</p>');
            $('html').addClass('reload');
            $('body').prepend($reload);

            reload = true;
        }

    }, 250);

    $(window).on('resize orientationchange', function(){
        destroyDesktop();
    });

});  // ready()

















