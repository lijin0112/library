            hash = '#recipes/0';
        }

        var hashparts = hash.split('/');
        var section = hashparts[0];
        var slide = hashparts[1];

        // Sync Slick Slider subnavigation with current slide
        if (section === '#people' && slide) {
            $('.people .nav_inner_list').slick('slickGoTo', slide);
        }

        if (section === '#recipes' && slide) {
            $('.cuisine_recipes .nav_inner_list').slick('slickGoTo', slide);
        }

        if (section === '#menu' || section === '#recipes') {
            section = '#cuisine';
        }

        // If hash isn't blank, then add/remove active classes
        if (hash !== '') {
            $('a[href="' + hash + '"], a[href=' + section + ']').closest('li').addClass('active');
            $('a:not([href="' + hash + '"], [href=' + section + '])').closest('li').removeClass('active');
        }

        // Deeplink to Partners section
        if (hash === '#partners') {
            // Go To About Section
            $.fn.fullpage.moveTo(2);

            // Open Partners section after 1s
            var openPartners = setTimeout(function(){
                $('#partners-content').collapse('show');
            }, 1000);
        }

