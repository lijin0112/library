    var debouncedScroll = debounce(function(){
        if(!no_more_tumblr_posts && $(this).scrollTop() + $(this).innerHeight() >= this.scrollHeight) {
            $('#loader').addClass('active');
            retrieveTumblrPosts(tumblr_offset);
        }
    }, 100);
    $('.section_gallery .inner').scroll(debouncedScroll);


    //
    // Get Tumblr Posts
    //
    function retrieveTumblrPosts(tumblr_post_offset) {
        //console.log('tumblr retrieve posts triggered');
        // If there are Tumblr posts to retrieve
        if(!no_more_tumblr_posts) {
            
            no_more_tumblr_posts = true;
            
            // Build the request for Tumblr posts
            var request;
            request = tumblr_api_root_path + "posts?callback=?&api_key=" + tumblr_api_key + "&limit=" + tumblr_posts_per_request + "&offset=" + tumblr_post_offset;
            if (tumblr_tag !== "all") {
                request += "&tag=" + tumblr_tag;
            }

            //
            // Call the Tumblr API
            //

            //console.debug(request);
            $.getJSON(request,function(data) {

                // Activate the loader indicator
                $('#loader.active').removeClass('active');

                // Get total posts data to know when we are at the end
                if (!tumblr_total_posts) {
                    tumblr_total_posts = data.response.total_posts;
                    // console.debug("Total Posts", tumblr_total_posts);
                }

                // console.debug("TUMBLR Data",data);
                // console.debug("TUMBLR Data",data.response.posts.length);

                //
                // Handle each post
                //
                $.each(data.response.posts, function(i, item) {
                    // console.debug(item);
                    switch(item.type) {
                        case "photo":
                            handleImageContent(item);
                            break;
                        case "video":
                            handleVideoContent(item);
                            break;
                        default:
                            handleDefaultContent(item);
                    }
                });

                // There are Tumblr posts left
                no_more_tumblr_posts = false;

                // If there are Tumblr posts left
                if ( (data.response.posts.length === tumblr_posts_per_request) && (tumblr_offset !== tumblr_total_posts) ) {
                    // Set tumblr_offset to the "next" set of posts
                    tumblr_offset += tumblr_posts_per_request;
                }
                else {
                    no_more_tumblr_posts = true;
                }

                //
                // If this is the first Tumblr request
                // Initialize Masonry
                //
                if (first_tumblr_request === true) {
                    
                    // Initialize Masonry
                    $grid.masonry({
                        itemSelector: '.grid-item',
                        columnWidth: '.grid-sizer',
                        percentPosition: true,
                        isInitLayout: false
                    });

                    first_tumblr_request = false;
                }

                // Manually initialize Masonry layout
                // every time getJSON is called
                $grid.masonry();

            });
        }
    }


    //
    // Handle Filter Clicks
    //
    function handleFilter($el) {
        // only do something if filter is changing
        if($el.data("tag") !== tumblr_tag) {
            // set the new active tag
            tumblr_tag = $el.data("tag");

            // Reset the Grid
            resetTumblrGrid();

            // Highlight current filter
            $(".tumblr-filter.active").removeClass("active");
            $el.addClass("active");

            // Get requested Tumblr posts
            retrieveTumblrPosts(tumblr_offset);

        }
    }

    //
    // Reset the Tumblr Grid
    //
    function resetTumblrGrid() {
        // reset the offset
        tumblr_offset = 0;

        // reset no more posts
        no_more_tumblr_posts = false;

        // Reset grid area
        // Get current .grid-item
        var gridItems = $('.grid-item');

        // remove masonry items
        $grid.masonry('remove', gridItems);

