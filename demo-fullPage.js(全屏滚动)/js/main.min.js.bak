    //
    // Handle Image Posts
    //
    function handleImageContent(item) {

        var image;
        var alt_sizes = item.photos[0].alt_sizes.length;
        var aspectRatio;
        var $newItem;

        if ( alt_sizes > 3 ) {
            image = item.photos[0].alt_sizes[2];
        } 
        else if ( alt_sizes > 2 ) {
            image = item.photos[0].alt_sizes[1];
        }
        else {
            image = item.photos[0].alt_sizes[0];
        }

        // determine aspect ratio size
        aspectRatio = (image.height / image.width) * 100 + '%';
        
        if(image.width !== image.height) {
            $newItem = $('<div class="grid-item grid-item--width-2"><div class="grid_item__inner" style="padding-bottom:'+ aspectRatio +';"><a target="_blank" href="'+item.post_url+'" ><img src="' + image.url + '" /></a></div></div>');
        } else {
            $newItem = $('<div class="grid-item grid-item--width-1"><div class="grid_item__inner" style="padding-bottom:100%;"><a target="_blank" href="'+item.post_url+'" ><img src="' + image.url + '" /></a></div></div>');
        }

        if (first_tumblr_request === true) {
            $grid.append($newItem);    
        } else {
            $grid.append($newItem).masonry('appended', $newItem);
        }
        
    }

    //
    // Handle Video Posts
    //
    function handleVideoContent(item) {
        var aspectRatio = (item.thumbnail_height / item.thumbnail_width) * 100 + '%';
        var $newItem;

        //console.debug("VIDEO", item);

        if (item.thumbnail_width !== item.thumbnail_height) {
            $newItem = $('<div class="grid-item grid-item--width-3"><div class="grid_item__inner" style="padding-bottom:'+ aspectRatio +';"><a target="_blank" href="'+item.post_url+'" ><img src="' + item.thumbnail_url + '" /></a></div></div>');
        } else {
            $newItem = $('<div class="grid-item grid-item--width-3"><div class="grid_item__inner" style="padding-bottom:100%;"><a target="_blank" href="'+item.post_url+'" ><img src="' + item.thumbnail_url + '" /></a></div></div>');
        }

        if (first_tumblr_request === true) {
            $grid.append($newItem);    
        } else {
            $grid.append($newItem).masonry('appended', $newItem);
        }
        
    }

    //
    // Handle Other Posts
    //
